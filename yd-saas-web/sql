-- Table structure for `yd_acquisition_plan`
-- ----------------------------
DROP TABLE IF EXISTS `yd_acquisition_plan`;
CREATE TABLE `yd_acquisition_plan` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `storehouse_id` bigint(20) NOT NULL COMMENT '组织id(这里只有仓库)',
  `storehouse_name` varchar(255) DEFAULT NULL COMMENT '仓库名称(冗余)',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `start_time` datetime DEFAULT NULL COMMENT '计划开始时间',
  `stop_time` datetime DEFAULT NULL COMMENT '计划截止时间',
  `acquisition_weight` varchar(255) DEFAULT NULL COMMENT '计划年收购数量(kg)',
  `create_user_id` bigint(20) DEFAULT NULL COMMENT '创建人id',
  `create_user_name` varchar(255) DEFAULT NULL COMMENT '创建人名称(冗余)',
  `smoke_station_name` varchar(255) DEFAULT NULL COMMENT '烟站名称',
  `smoke_station_id` bigint(20) DEFAULT NULL COMMENT '烟站id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='收购计划';

