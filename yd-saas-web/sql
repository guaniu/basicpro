  `rfid` varchar(255) DEFAULT NULL COMMENT '文山扣号',
  `tobacco_id` int(11) DEFAULT NULL COMMENT '烟叶信息id',
  `variety` varchar(255) DEFAULT NULL COMMENT '品种',
  `package_date` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '打包时间',
  `package_type` varchar(255) DEFAULT NULL COMMENT '打包类型（0烟包1烟框）',
  `count` double(16,2) DEFAULT NULL COMMENT '出/入库数量',
  `unit` varchar(255) DEFAULT NULL COMMENT '单位',
  `weight` varchar(255) DEFAULT NULL COMMENT '重量（kg 担转kg）',
  `weight_log` varchar(255) DEFAULT NULL COMMENT '重量（担）',
  `tobacco_station` varchar(255) DEFAULT NULL COMMENT '所属烟站（发货单位）',
  `repet_tobacco_station` varchar(255) DEFAULT NULL COMMENT '所属烟站(收货单位)',
  `status` int(4) DEFAULT NULL COMMENT '0 烟站仓库 1复烤厂仓库',
  `sugar` varchar(255) DEFAULT NULL COMMENT '总糖',
  `nicotine` varchar(255) DEFAULT NULL COMMENT '烟碱',
  `nitrogen` varchar(255) DEFAULT NULL COMMENT '总氮',
  `protein` varchar(255) DEFAULT NULL COMMENT '蛋白质',
  `create_by` varchar(255) DEFAULT NULL COMMENT '创建人',
  `create_date` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `is_valid` int(4) DEFAULT NULL COMMENT '是否有效(0有效1无效)',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=30 DEFAULT CHARSET=utf8;

