DROP TABLE IF EXISTS `yd_warn`;
CREATE TABLE `yd_warn` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `warn_type` varchar(255) DEFAULT NULL COMMENT '告警类型:1:停车超时2:低电量3:丢失4:拆卸',
  `transport_id` int(11) DEFAULT NULL COMMENT '运单Id',
  `car_longitude` varchar(255) DEFAULT NULL COMMENT '车辆经度',
  `car_latitude` varchar(255) DEFAULT NULL COMMENT '车辆维度',
  `warn_content` varchar(255) DEFAULT NULL COMMENT '告警内容',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime DEFAULT NULL COMMENT '修改时间',
  `process_state` int(11) DEFAULT '0' COMMENT '处理状态:0未处理1:已处理',
  `process_user_id` bigint(20) DEFAULT NULL COMMENT '处理人id',
  `process_user_name` varchar(255) DEFAULT NULL COMMENT '处理人名称',
  `process_content` varchar(255) DEFAULT NULL COMMENT '处理结果说明',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8 COMMENT='告警表';

