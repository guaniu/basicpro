-- ----------------------------
-- Table structure for `yd_organization`
-- ----------------------------
DROP TABLE IF EXISTS `yd_organization`;
CREATE TABLE `yd_organization` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `org_name` varchar(255) DEFAULT NULL COMMENT '组织名称',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `parent_id` bigint(20) NOT NULL COMMENT '父id',
  `org_full_name` varchar(255) DEFAULT NULL COMMENT '全称',
  `order_num` int(11) DEFAULT NULL COMMENT '排序字段',
  `org_code` varchar(255) DEFAULT NULL COMMENT '企业编码',
  `org_type` varchar(255) NOT NULL COMMENT '组织类型1:企业2:烟站3:复烤厂4:仓库',
  `org_district_id` int(255) DEFAULT NULL COMMENT '归属区域id',
  `org_longitude` varchar(255) DEFAULT NULL COMMENT '经度',
  `org_latitude` varchar(255) DEFAULT NULL COMMENT '纬度',
  `org_address` varchar(500) DEFAULT NULL COMMENT '公司具体地址',
  `org_tele` varchar(255) DEFAULT NULL COMMENT '电话',
  `offical_user_id` bigint(20) DEFAULT NULL COMMENT '公司负责人id',
  `offical_user_name` varchar(255) DEFAULT NULL COMMENT '公司负责人名称',
  `org_comments` mediumtext COMMENT '组织备注信息',
  `org_status` int(6) NOT NULL DEFAULT '1' COMMENT '状态:1正常0停用',
  `is_del` int(11) NOT NULL DEFAULT '0' COMMENT '0:未删除1:删除',
  `storehouse_volume` varchar(255) DEFAULT NULL COMMENT '仓库容积(单位:千克)',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8 COMMENT='组织机构';

