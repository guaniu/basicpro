-- ----------------------------
-- Table structure for `yd_information`
-- ----------------------------
DROP TABLE IF EXISTS `yd_information`;
CREATE TABLE `yd_information` (
  `id` int(10) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `transport_id` int(10) DEFAULT NULL COMMENT '运单id',
  `user_id` int(10) DEFAULT NULL COMMENT '用户id',
  `type` int(2) DEFAULT NULL COMMENT '类型： 1.停车超时 2.电量低 3.丢失 4.拆卸 5.车辆出发 6.待收货 7.已收货',
  `content` varchar(255) DEFAULT NULL COMMENT '内容',
  `create_time` datetime DEFAULT NULL,
  `is_read` int(1) DEFAULT '1' COMMENT '是否已读：1.未读 2.已读',
  `status` int(1) DEFAULT '0' COMMENT '是否有效：0.有效 1.无效',
  `parent_ids` varchar(255) DEFAULT NULL COMMENT '所属上级机构标识',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 COMMENT='消息表';

