-- ----------------------------
-- Table structure for `yd_transport`
-- ----------------------------
DROP TABLE IF EXISTS `yd_transport`;
CREATE TABLE `yd_transport` (
  `id` int(10) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `org_id` int(10) NOT NULL COMMENT '组织机构id',
  `send_id` int(10) NOT NULL COMMENT '发货方id',
  `send_name` varchar(50) NOT NULL COMMENT '发货方名称',
  `receive_id` int(10) NOT NULL COMMENT '收货方id',
  `receive_name` varchar(50) NOT NULL COMMENT '收货方名称',
  `device_no` varchar(20) NOT NULL COMMENT '车载设备编号',
  `car_no` varchar(20) NOT NULL COMMENT '车牌号',
  `dispatch_no` varchar(255) NOT NULL COMMENT '调度单号',
  `transport_no` varchar(255) NOT NULL COMMENT '运单号',
  `estime` int(10) DEFAULT NULL COMMENT '预计用时(秒)',
  `driver_name` varchar(20) DEFAULT NULL COMMENT '驾驶员姓名',
  `driver_mobile` varchar(20) DEFAULT NULL COMMENT '驾驶员手机号',
  `transport_state` int(2) DEFAULT '1' COMMENT '运单状态 1.待装车 2.已装车 3.在途中 4.待收货5.已收货',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `depart_time` datetime DEFAULT NULL COMMENT '出发时间',
  `arrive_time` datetime DEFAULT NULL COMMENT '到达时间',
  `finsh_time` datetime DEFAULT NULL COMMENT '完成时间',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `status` int(1) DEFAULT '0' COMMENT '状态：0.有效 1.无效',
  `remarks` varchar(2000) DEFAULT NULL COMMENT '备注',
  `loader_id` int(10) DEFAULT NULL COMMENT '装车读写设备id',
  `weight` varchar(20) DEFAULT NULL COMMENT '总重(担)',
  `goods_type` varchar(10) DEFAULT NULL COMMENT '物品类型(烟框/烟包)',
  `goods_num` int(10) DEFAULT NULL COMMENT '物品数量',
  `parent_ids` varchar(255) DEFAULT NULL COMMENT '所属上级机构标识',
  PRIMARY KEY (`id`),
  KEY `orgIds_index` (`parent_ids`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=64 DEFAULT CHARSET=utf8 COMMENT='运单表';

