-- Table structure for `yd_basics_warehouse`
-- ----------------------------
DROP TABLE IF EXISTS `yd_basics_warehouse`;
CREATE TABLE `yd_basics_warehouse` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `tobacco_id` int(11) DEFAULT NULL COMMENT '烟叶信息id',
  `district_id` int(11) DEFAULT NULL COMMENT '地区id',
  `org_id` int(11) DEFAULT NULL COMMENT '组织id（仓库id）',
  `org_ids` varchar(255) DEFAULT NULL COMMENT '组织id集合（用于数据权限）',
  `package_type` varchar(255) DEFAULT NULL COMMENT '打包类型（0烟包1烟框）',
  `weight_one` varchar(255) DEFAULT NULL COMMENT '重量（kg）（库存）',
  `unit_one` varchar(255) DEFAULT NULL COMMENT '单位（kg）',
  `weight_two` varchar(255) DEFAULT NULL COMMENT '重量（担）（库存）',
  `unit_two` varchar(255) DEFAULT NULL COMMENT '单位（担）',
  `tobacco_station` varchar(255) DEFAULT NULL COMMENT '所属烟站（发货单位/收货单位）',
  `capacity` varchar(255) DEFAULT NULL COMMENT '仓库总容量',
  `package_date` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '打包时间',
  `create_by` varchar(255) DEFAULT NULL COMMENT '创建人',
  `create_date` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `status` int(4) DEFAULT NULL COMMENT '0 烟站仓库 1复烤厂仓库',
  `is_valid` int(4) DEFAULT NULL COMMENT '0有效1无效',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8;

